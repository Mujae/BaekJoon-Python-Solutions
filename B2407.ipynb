    "def f(n):\n",
    "    num = 1\n",
    "    for i in range(2,n+1):\n",
    "        num*=i\n",
    "    return num\n",
    "\n",
    "n,m = map(int, input().split())\n",
    "print(f(n) // (f(m)*f(n-m)))"
   ]
