   "source": [
    "n = int(input())\n",
    "li = []\n",
    "c=0\n",
    "\n",
    "while n > 0:\n",
    "    li.append(n % 2)\n",
    "    n //= 2\n",
    "    \n",
    "for i in range(len(li)):\n",
    "    if li[i] == 1:\n",
    "        c += 3 ** i\n",
    "print(c)"
   ]
