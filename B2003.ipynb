   "source": [
    "n, m = map(int, input().split())\n",
    "li = list(map(int, input().split()))\n",
    "\n",
    "c = 0\n",
    "s = 0\n",
    "end = 0\n",
    "\n",
    "for i in range(n):\n",
    "    while s < m and end < n:\n",
    "        s += li[end]\n",
    "        end += 1\n",
    "    if s == m:\n",
    "        c += 1\n",
    "    s -= li[i] \n",
    "print(c)"
   ]
  }
