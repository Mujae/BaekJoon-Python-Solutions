   "source": [
    "n= int(input())\n",
    "\n",
    "s=str(n)\n",
    "\n",
    "l=len(s)\n",
    "c=0\n",
    "for i in range(l):\n",
    "    w=int(s[i])\n",
    "    if w>4:\n",
    "        c+=(w-1)*(9**(l-i-1))\n",
    "    else:\n",
    "        c+=w*(9**(l-i-1))\n",
    "\n",
    "print(c)\n"
   ]
  }
