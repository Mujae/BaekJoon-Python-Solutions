   "source": [
    "import math\n",
    "from decimal import Decimal\n",
    "x, y = map(int,input().split())\n",
    "z=math.floor(Decimal(str(y/x))*100)\n",
    "if z>=99:\n",
    "    print(-1)\n",
    "else:\n",
    "    print(math.ceil((x*(z+1)-100*y)/(99-z)))"
   ]
  }
