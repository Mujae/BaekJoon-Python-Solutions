   "source": [
    "n,k = map(int,input().split())\n",
    "for i in range(1,n+1):\n",
    "    if n % i == 0:\n",
    "        k -= 1\n",
    "    if k == 0:\n",
    "        print(i)\n",
    "        break\n",
    "else:\n",
    "    print(0)"
   ]
